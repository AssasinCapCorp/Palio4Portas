        app:layout_constraintEnd_toEndOf="@+id/quarta"
        app:layout_constraintHorizontal_bias="0.32"
        app:layout_constraintStart_toStartOf="@+id/quarta"
        app:layout_constraintTop_toTopOf="@+id/DSTQQSS"
        app:srcCompat="@drawable/retanguloseletor" />

    <ImageView
        android:id="@+id/daySelectorQuinta"
        android:layout_width="39dp"
        android:layout_height="92dp"
        android:visibility="invisible"
