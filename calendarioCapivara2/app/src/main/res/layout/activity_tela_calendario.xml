    <TextView
        android:id="@+id/terca"
        android:layout_width="39dp"
        android:layout_height="35dp"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="5dp"
        android:text="11"
        android:textAlignment="center"
        android:textColor="#DDDFEA"
        android:textSize="26sp"
        app:layout_constraintBottom_toBottomOf="@+id/linhaCinza"
        app:layout_constraintEnd_toEndOf="@+id/linhaCinza"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/segunda"
        app:layout_constraintTop_toTopOf="@+id/linhaCinza" />

    <TextView
        android:id="@+id/quarta"
        android:layout_width="39dp"
        android:layout_height="35dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="5dp"
        android:text="11"
        android:textAlignment="center"
        android:textColor="#DDDFEA"
        android:textSize="26sp"
        app:layout_constraintBottom_toBottomOf="@+id/linhaCinza"
        app:layout_constraintEnd_toEndOf="@+id/linhaCinza"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/terca"
        app:layout_constraintTop_toTopOf="@+id/linhaCinza"
        app:layout_constraintVertical_bias="0.6" />

    <TextView
        android:id="@+id/quinta"
        android:layout_width="39dp"
        android:layout_height="35dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="5dp"
        android:text="11"
        android:textAlignment="center"
        android:textColor="#DDDFEA"
        android:textSize="26sp"
        app:layout_constraintBottom_toBottomOf="@+id/linhaCinza"
        app:layout_constraintEnd_toEndOf="@+id/linhaCinza"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/quarta"
        app:layout_constraintTop_toTopOf="@+id/linhaCinza" />

    <TextView
        android:id="@+id/sexta"
        android:layout_width="39dp"
        android:layout_height="35dp"
