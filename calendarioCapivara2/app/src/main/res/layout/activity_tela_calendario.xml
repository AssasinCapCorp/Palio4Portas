        app:srcCompat="@drawable/linha_dos_dias" />

    <ImageView
        android:id="@+id/daySelectorDomingo"
        android:layout_width="39dp"
        android:layout_height="92dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/domingo"
        app:layout_constraintEnd_toEndOf="@+id/domingo"
        app:layout_constraintHorizontal_bias="0.32"
