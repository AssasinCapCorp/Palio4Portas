        android:textColor="#DDDFEA"
        android:textSize="26sp"
        app:layout_constraintBottom_toBottomOf="@+id/daySelectorSexta"
        app:layout_constraintEnd_toEndOf="@+id/linhaCinza"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/sexta"
